<!doctype html>
<html lang="en">
 
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="assets/vendor/bootstrap/css/bootstrap.min.css">
    <link href="assets/vendor/fonts/circular-std/style.css" rel="stylesheet">
    <link rel="stylesheet" href="assets/libs/css/style.css">
    <link rel="stylesheet" href="assets/vendor/fonts/fontawesome/css/fontawesome-all.css">
    <script src="assets/libs/js/jquery-3.5.1.min.js" crossorigin="anonymous"></script>
    <title>Atkoinc :: Account</title>
    <link rel="icon" type="image/png" href="assets/images/icons/logo.png">
</head>
<body>
    <!-- ============================================================== -->
    <!-- main wrapper -->
    <!-- ============================================================== -->
    <div class="dashboard-main-wrapper">
        <!-- ============================================================== -->
        <!-- wrapper  -->
        <!-- ============================================================== -->
        {{>navbar}}
        {{>sidebar}}
        <div class="dashboard-wrapper">
            <div class="container-fluid dashboard-content">
                {{>tokenStore}}
                {{>flash}}
                {{{body}}}
            </div>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- end main wrapper  -->
    <!-- ============================================================== -->
    <script src="assets/vendor/bootstrap/js/bootstrap.bundle.js"></script>
    <script src="assets/libs/js/main-js.js"></script>

    <script>
        // Auto set the active menu item
        var links = $('#navbarNav .nav-link');
        for (var i = 0; i < links.length; i++) {
            if (links[i].href == document.URL) {
                $(links[i]).addClass('active').parents('.submenu').removeClass('collapse').addClass('expand');
                $(links[i])
            }
        }
    </script>
</body>
 
</html>        